<dakota>
 <version>0.0.0.1</version>
 <resourceMap>
  <define_resource>
   <name>red</name>
   <color>
    <r>0.75</r>
    <g>0.1</g>
    <b>0.1</b>
    <a>1</a>
   </color>
  </define_resource>
  <define_resource>
   <name>green</name>
   <color>
    <r>0.25</r>
    <g>0.80</g>
    <b>0.25</b>
    <a>1</a>
   </color>
  </define_resource>
  <define_resource>
   <name>blue</name>
   <color>
    <r>0.05</r>
    <g>0.35</g>
    <b>0.95</b>
    <a>1</a>
   </color>
  </define_resource>
 </resourceMap>
 <window>
  <title>Dakota Test</title>
  <window_width>512</window_width>
  <window_height>512</window_height>
 </window>
 <layout>
  <el>
   <el>
    <resource>red</resource>
    <size>
     <width><constant>340</constant></width>
     <height><constant>340</constant></height>
    </size>
   </el>
   <el>
    <resource>green</resource>
    <size>
     <width><constant>64</constant></width>
     <height><constant>64</constant></height>
    </size>
   </el>
   <el>
    <resource>blue</resource>
    <size>
     <width><constant>256</constant></width>
     <height><constant>256</constant></height>
    </size>
   </el>
  </el>
 </layout>
</dakota>
