<dakota>
 <version>0.0.0.1</version>
 <resourceMap>
  <define_resource>
   <name>cursor</name>
   <image>
    <format>ARGB8888</format>
    <data>
     <relPath>images/cursor.png</relPath>
    </data>
   </image>
   <hints>
    <static>true</static>
   </hints>
  </define_resource>
  <define_resource>
   <name>samson</name>
   <image>
    <format>ARGB8888</format>
    <data>
     <relPath>images/brock.png</relPath>
    </data>
   </image>
  </define_resource>
  <define_resource>
   <name>blue</name>
   <color>
    <r>0.05</r>
    <g>0.35</g>
    <b>0.95</b>
    <a>1</a>
   </color>
  </define_resource>
 </resourceMap>
 <window>
  <title>Dakota Test</title>
  <window_width>512</window_width>
  <window_height>512</window_height>
 </window>
 <layout>
  <el>
   <el>
    <resource>samson</resource>
    <offset>
     <x><constant>256</constant></x>
     <y><constant>256</constant></y>
    </offset>
    <size>
     <width><constant>128</constant></width>
     <height><constant>128</constant></height>
    </size>
   </el>
   <el>
    <resource>blue</resource>
    <offset>
     <x><constant>64</constant></x>
     <y><constant>64</constant></y>
    </offset>
    <size>
     <width><constant>256</constant></width>
     <height><constant>256</constant></height>
    </size>
    <el>
     <unbounded_subsurface></unbounded_subsurface>
     <resource>cursor</resource>
     <offset>
      <x><constant>220</constant></x>
      <y><constant>64</constant></y>
     </offset>
     <size>
      <width><constant>128</constant></width>
      <height><constant>128</constant></height>
     </size>
    </el>
   </el>
  </el>
 </layout>
</dakota>
